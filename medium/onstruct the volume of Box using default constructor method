class Box {
    double length, width, height;
    Box() {
        length = 5.0;
        width = 3.0;
        height = 2.0;
    }
    double volume() {
        return length * width * height;
    }
}
public class Main {
    public static void main(String[] args) {
        Box box = new Box();
        System.out.println("Volume of the box: " + box.volume());
    }
}
