        System.out.println(" X         X ");
        System.out.println(" X  X   X  X ");
        System.out.println(" X   XXX   X ");
        System.out.println("  X       X  ");
        System.out.println("   XXXXXXX   ");
        System.out.println("     XXX     ");
    }
}
