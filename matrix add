class matadd 
{
    public static void main(String[] args) 
    {
        int a[][]={{1,2},{3,4}}, b[][]={{1,2},{3,4}},c[][]={{0,0},{0,0}},n=2;
        for (int i = 0; i < n; i++) 
        {
            for (int j = 0; j < n; j++) 
              {  
                  c[i][j] = a[i][j] +b[i][j];
                  System.out.print(c[i][j] + " ");
              }
            System.out.println("");
        }
    }
}
